<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="RegisterTextInputLayoutStyle" parent="Widget.MaterialComponents.TextInputLayout.FilledBox">
        <item name="boxBackgroundMode">filled</item>
        <item name="boxBackgroundColor">@color/unselected_white</item>
        <item name="boxStrokeColor">@color/unselected_white</item>
        <item name="hintTextColor">@color/unselected_white</item>
        <item name="android:textColorHint">@color/unselected_white</item>
        <item name="android:textColor">@color/unselected_white</item>
    </style>

    <style name="TitleToolbarTextAppearance" parent="TextAppearance.Material3.BodyMedium">
        <item name="fontFamily">@font/roboto_medium</item>
        <item name="android:fontFamily">@font/roboto_medium</item>
        <item name="android:textSize">20sp</item>
        <item name="textFillColor">@color/white</item>
        <item name="android:textAllCaps">false</item>
    </style>
</resources>